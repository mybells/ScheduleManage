{
  "name": "schedulemanage-server",
  "version": "0.1.0",
  "description": "node server",
  "private": true,
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "client": "npm run serve --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.1",
    "express": "^4.17.1",
    "mongodb": "^3.3.0-beta2",
    "mongoose": "^5.7.5"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  }
}
